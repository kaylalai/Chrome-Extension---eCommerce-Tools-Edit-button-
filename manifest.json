{
    "name": "Pathway to Neto Edit function ",
    "version": "0.0.1",
    "description": "This is the edit button plugin work for most of the neto store",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "./index.html",
        "default_title": "Open the popup",
        "default_icon": "./icon.png"
    },
    "content_scripts": [{
        "js": [
            "content.js"
        ],
        "matches": [
            "<all_urls>"
        ]
    }],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "<all_urls>"
    ]
}