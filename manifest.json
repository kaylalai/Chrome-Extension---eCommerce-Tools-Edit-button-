{
    "name": "Pathway to Neto Edit function ",
    "version": "0.0.1",
    "description": "This is the edit button plugin work for most of the neto store",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "./index.html",
        "default_title": "Open the popup",
        "default_icon": "icon-128.png",
        "icon": {
            "32": "icon-32.png",
            "48": "icon-48.png",
            "128": "icon-128.png"
        }
    },
    "content_scripts": [{
        "js": [
            "content.js"
        ],
        "matches": [
            "<all_urls>"
        ]
    }],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "<all_urls>"
    ]

}
